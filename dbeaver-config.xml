<?xml version="1.0" encoding="UTF-8"?>
<cloudbeaver xmlns="http://cloudbeaver.io/schema/config"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://cloudbeaver.io/schema/config config.xsd">
  <connections>
    <connection name="My Database Connection"
                driver="org.postgresql.Driver"
                url="jdbc:postgresql://db:${PGPORT}/${PGDB}"
                username="${PGUSER}"
                password="${PGPASSWORD}" />
  </connections>
</cloudbeaver>